{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint", "my-eslint-plugin"],
  "rules": {
    "no-console": 1,
    "quotes": ["error", "single"],
    "my-eslint-plugin/no-window-config": "error"
  }
}
